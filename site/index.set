        #menuToggle {
            display: block;
            position: fixed;
            right: 0;
            top: 5%;
            right: 2%;
            z-index: 50;
            -webkit-user-select: none;
            user-select: none;
        }
        #menuToggle:hover {
            zoom: 1;
            filter: alpha(opacity=100);
            opacity: 1.0;
            transition: 0.15s ease-in;
        }
        #menuToggle input {
            display: block;
            width: 40px;
            height: 32px;
            position: absolute;
            top: -7px;
            left: -5px;
            cursor: pointer;
            opacity: 0;
            z-index: 2;
            -webkit-touch-callout: none;
        }
        #menuToggle span {
            display: block;
            width: 33px;
            height: 4px;
            margin-bottom: 5px;
            position: relative;
            background-color: #333333;
            z-index: 1;
            opacity: 0.6;
            transform-origin: 4px 0px;
            transition: transform 0.0s cubic-bezier(0.77, 0.2, 0.05, 1.0), background 0.0s cubic-bezier(0.77, 0.2, 0.05, 1.0), opacity 0.25s ease;
            box-shadow: 1px -1px 0px 0px rgba(230, 230, 230, .3), 1px -1px 2px 2px rgba(184, 184, 184, .2), 2px -1px 3px 3px rgba(174, 174, 174, .1), -1px 1px 1px 1px rgba(140, 140, 140, .1), -1px 1px 2px 2px rgba(80, 80, 80, .05), -1px 1px 3px 3px rgba(80, 80, 80, .025);
            transition: 0.3s;
        }
        #menuToggle span:first-child {
            transform-origin: 0% 0%;
            transition: 0.3s;
        }
        #menuToggle span:nth-last-child(2) {
            transform-origin: 0% 100%;
            transition: 0.3s;
        }
        #menuToggle input:checked~span {
            transform: rotate(45deg) translate(-2px, -1px) scale(0.25, 1);
            transition: 0.45s;
            opacity: 1;
            background-color: #333333;
            /*
	-webkit-box-shadow: 0px -0px 0px 0px rgba(230, 230, 230, 1);
	*-moz-box-shadow: 0px -0px 0px 0px rgba(230, 230, 230, 1);*
	box-shadow: 0px 0px 0px 0px rgba(230, 230, 230, .0);
		*/
        }
        #menuToggle input:checked~span:nth-last-child(3) {
            opacity: 1;
            transform: translate(12px, 1px) rotate(45deg) scale(.25, 1);
            transition: 0.45s;
            background-color: #333333;
        }
        #menuToggle input:checked~span:nth-last-child(2) {
            opacity: 1;
            transform: rotate(-45deg) translate(0, -1px) scale(0.585, 1);
            transition: 0.45s;
            background-color: #333333;
        }
