        }
        .accordion-item-hd:hover {
            transition: text-shadow .4s ease-in-out, box-shadow .4s ease-in-out;
            color: rgb(60, 145, 255);
            text-shadow: 0px 1px 1px rgba(255, 255, 255, 0.53), 0px 0px 4px rgba(110, 110, 255, 0.83);
            box-shadow: 0px 0px 15px 15px rgba(42, 177, 255, 0.1), 1px -1px 1px 2px rgba(227, 231, 233, 0.5), 3px -3px 3px 4px rgba(179, 184, 185, 0.7);
        }
        .accordion-item-input:checked~.accordion-item-bd {
            max-height: 5000px;
            padding-top: 15px;
            margin-bottom: 15px;
            -webkit-transition: max-height .3s ease-in, margin .3s ease-in, padding .3s ease-in;
            transition: max-height .3s ease-in, margin .3s ease-in, padding .3s ease-in;
        }
        .accordion-item-input:checked~.accordion-item-bd>#detailsdiv code,
        .accordion-item-input:checked~.accordion-item-bd>#detailsdiv p {
            /*background-color: #515151;*/
            transition-delay: 1.5s;
            transition: color .8s ease-in-out, text-shadow .8s ease-in-out;
            -webkit-transition-delay: 1.5s;
            color: white;
            text-shadow: -1px 1px 1px rgba(0, 0, 0, 0.23);
        }
        .accordion-item-input:checked~.accordion-item-bd>#detailsdiv h4,
        .accordion-item-input:checked~.accordion-item-bd>#detailsdiv h3 {
            transition-delay: 1s;
            transition: text-shadow, .4s ease-in-out, color .4s ease-in-out;
            -webkit-transition-delay: 1s;
            color: white;
            text-shadow: -1px 1px 1px rgba(0, 0, 0, 0.23);
        }
        .accordion-item-input:checked~.accordion-item-hd>.accordion-item-hd-cta {
            -webkit-transform: rotate(0);
            -ms-transform: rotate(0);
            transform: rotate(0);
        }
        .accordion-item-hd-cta {
            display: block;
            width: 30px;
            position: absolute;
            top: calc(50% - 6px);
            /*minus half font-size*/
            right: 0;
            pointer-events: none;
            -webkit-transition: -webkit-transform .3s ease;
